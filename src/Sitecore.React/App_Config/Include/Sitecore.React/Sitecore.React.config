<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
	<sitecore>
		<settings>
			<setting name="React.DynamicPlaceholders.PredictableNames" value="false"></setting>
			<setting name="React.BundleName" value="~/bundles/react"></setting>
			<setting name="" value=""></setting>
			
		</settings>
    <pipelines>
      <mvc.getPageRendering>
        <processor
          patch:before="*[@type='Sitecore.Mvc.Pipelines.Response.GetPageRendering.GetLayoutRendering, Sitecore.Mvc']"
          type="Sitecore.Foundation.React.Pipelines.GetPageRendering.AddJsxFiles, Sitecore.Foundation.React">
          <defaultAssets hint="raw:AddAsset">
          </defaultAssets>
        </processor>
      </mvc.getPageRendering>
    </pipelines>
	</sitecore>
</configuration>